	border: 1px solid rgb(246, 96, 0);
	border-radius: 2px;
}

QLineEdit:disabled, QTextEdit:disabled, QPlainTextEdit:disabled {
	color: rgb(50, 50, 50);
	background: rgb(40, 40, 40);
	border: 1px solid rgb(50, 50, 50);
	border-radius: 2px;
}

