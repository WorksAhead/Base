QScrollBar:horizontal {
	background-color: rgb(50, 50, 50);
	height: 20px;
	margin: 0px 20px 0 20px;
	border: 0;
}
QScrollBar::handle:horizontal {
	background-color: rgb(80, 80, 80);
	min-width: 20px;
	border-top: 5px solid rgb(50, 50, 50);
	border-bottom: 5px solid rgb(50, 50, 50);
}
QScrollBar::add-line:horizontal {
	background-color: rgb(50, 50, 50);
    width: 20px;
    subcontrol-position: right;
    subcontrol-origin: margin;
    border: 0;
}
QScrollBar::sub-line:horizontal {
	background-color: rgb(50, 50, 50);
    width: 20px;
    subcontrol-position: left;
    subcontrol-origin: margin;
    border: 0;
}
QScrollBar:left-arrow:horizontal {
    width: 20px;
    height: 20px;
	image: url(:/Icons/IndicatorArrowLeft.png);

}
QScrollBar::right-arrow:horizontal {
    width: 20px;
    height: 20px;
	image: url(:/Icons/IndicatorArrowRight.png);
}
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
    background: none;
}
QScrollBar:vertical {
	background-color: rgb(50, 50, 50);
	width: 20px;
	margin: 20px 0 20px 0;
	border: 0;
}
QScrollBar::handle:vertical {
	background-color: rgb(80, 80, 80);
	min-height: 20px;
	border-left: 5px solid rgb(50, 50, 50);
	border-right: 5px solid rgb(50, 50, 50);
}
QScrollBar::add-line:vertical {
	background-color: rgb(50, 50, 50);
    height: 20px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
    border: 0;
}
QScrollBar::sub-line:vertical {
	background-color: rgb(50, 50, 50);
    height: 20px;
    subcontrol-position: top;
    subcontrol-origin: margin;
    border: 0;
}
QScrollBar::up-arrow:vertical {
    width: 20px;
    height: 20px;
	image: url(:/Icons/IndicatorArrowUp.png);
}
QScrollBar::down-arrow:vertical {
    width: 20px;
    height: 20px;
	image: url(:/Icons/IndicatorArrowDown.png);
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: none;
}
QAbstractScrollArea::corner {
	background-color: rgb(50, 50, 50);
}

QPushButton:flat {
	border: 0;
}

QPushButton:!flat {
	color: rgb(250, 250, 250);
	background: rgb(60, 60, 60);
	border: 1px solid rgb(80, 80, 80);
	border-radius: 2px;
	padding: 2px 5px 2px 5px;
}
QPushButton:!flat:focus {
	border: 1px solid rgb(246, 96, 0);
}
QPushButton:!flat:pressed {
	background: rgb(246, 96, 0);
	border: 1px solid rgb(246, 96, 0);
}
QPushButton:!flat:disabled {
	color: rgb(127, 127, 127);
}

QToolButton {
	color: rgb(250, 250, 250);
	background: rgb(60, 60, 60);
    border: 1px solid rgb(80, 80, 80);
    border-radius: 2px;
}
QToolButton::menu-indicator {
    image: url(:/Icons/IndicatorArrowDown.png);
    subcontrol-origin: padding;
    subcontrol-position: bottom right;
}
QToolButton[popupMode="1"],
QToolButton[popupMode="2"] {
    padding-right: 20px;
}
QToolButton:pressed {
    background: rgb(246, 96, 0);
	border: 1px solid rgb(246, 96, 0);
}
QToolButton:open {
	background: rgb(60, 60, 60);
	border: 1px solid rgb(246, 96, 0);	
}
QToolButton::menu-button {
	/*border-left: 1px solid rgb(40, 40, 40);*/
	border: 0;
    width: 16px;
}

QToolTip {
	color: rgb(250, 250, 250);
	background: rgb(30, 30, 30);
	border: 2px solid rgb(127, 127, 127);
}

QComboBox {
	color: rgb(250, 250, 250);
	background: rgb(60, 60, 60);
	border: 1px solid rgb(80, 80, 80);
	border-radius: 2px;
	padding: 2px 5px 2px 5px;
}
QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 15px;
    border-top-right-radius: 2px;
    border-bottom-right-radius: 2px;
}
QComboBox::down-arrow {
    image: url(:/Icons/IndicatorArrowDown.png);
}

QMenu {
    background-color: rgb(60, 60, 60);
    border: 1px solid rgb(80, 80, 80);
}
QMenu::item {
	color: rgb(250, 250, 250);
    background-color: transparent;
}
QMenu::item:selected {
    background-color: rgb(246, 96, 0);
}

